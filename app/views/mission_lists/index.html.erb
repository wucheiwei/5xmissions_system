<h1>任務管理列表</h1>
<%= render "form" %>

  <%@mission.each do |mission|%>
  <li>
    任務內容：<%=mission.task%>
    開始時間：<%=mission.start_date%>
    結束時間：<%=mission.due_date%>
    <%=link_to "編輯",edit_mission_list_path(mission)%>
    <%=link_to "刪除",mission_list_path(mission),method:"delete",data: {confirm: "確定嗎"}%>
  </li>
  <%end%>

<%=link_to "建立任務",new_mission_list_path%>
